<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="UIC Rule"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="Run UIC"
			CommandLine="$(QTBIN)\uic.exe $(InputPath) -o $(BuildDir)\Uic\$(ProjectName)\ui_$(InputName).h"
			Outputs="$(BuildDir)\Uic\$(ProjectName)\ui_$(InputName).h"
			FileExtensions="*.ui"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
